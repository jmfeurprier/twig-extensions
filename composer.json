{
    "name" : "jmf/twig-extensions",
    "description" : "Collection of Twig extensions",
    "license": "mit",
    "type": "library",
    "require": {
        "php": ">=8.3",
        "symfony/property-access": "^6.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phing/phing": "^2.17",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1",
        "rector/rector": "^0.19.5",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "suggest": {
        "ext-intl": "Required for localized date and time formatting."
    },
    "config": {
        "audit": {
            "abandoned": "report"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jmf\\TwigExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmf\\TwigExtensions\\Tests\\": "tests/"
        }
    }
}
